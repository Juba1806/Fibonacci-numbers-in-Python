# making a directory to memoialze
fioniacci ={}
def fibonacci(x):
    if x in  fioniacci:
        return fioniacci[x]
    # Compute the Nth term
    if x == 1 or x ==2:
        return 2
    elif x > 2:
        value = fibonacci(x-1) + fibonacci(x-2)
    fioniacci[x] = value
    return value
for x in range(1,200):
    print(x, ":" , fibonacci(x))
